<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HabitTracking.MainPage"
             Title="Category">

    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="AddCategory" Text="Category" Clicked="AddCategory_Clicked"></Button>
            <Button x:Name="AddN" Text="Add Habit" Clicked="AddN_Clicked"></Button>
            <Button x:Name="AddB" Text="Statistic" Clicked="AddB_Clicked"></Button>
            <Button x:Name="AddE" Text="Edit Habit" Clicked="AddE_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>

</ContentPage>